<style>
  .footer-container {
    background-color: var(--dark-blue);
    padding: 2rem;
    .container {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(20rem, 1fr));
      gap: 3rem;
    }

    * {
      color: var(--dark-blue);
    }

    h4 {
      margin-bottom: 2rem;
    }

    p {
      margin-bottom: 1rem;
      line-height: 1.6rem;
    }

    .form-container {
      form {
        display: flex;
        border-bottom: 1px solid var(--dark-blue);
        input {
          border: none;
          background: transparent;
          padding: 10px;

          &:active {
            border: none;
            outline: none;
          }
          &:hover {
            border: none;
            outline: none;
          }
        }
        input[type="submit"] {
          cursor: pointer;
          transition: all ease-in-out 0.25s;

          &:hover {
            opacity: 0.6;
          }
        }
      }
    }

    .icon-containers {
      display: flex;
      gap: 1rem;

      a {
        transition: all 0.3s ease;
      }

      &:hover {
        a {
          transform: scale(1.05) translatey(-5%);
          &:not(:hover) {
            opacity: 0.7;
            transform: scale(0.85);
          }
        }
      }
    }

    .links {
      position: relative;
      li {
        position: relative;
        margin-bottom: 10px;
        width: max-content;
      }
      a {
        padding: 5px 0 5px 0;

        display: block;
        width: 100%;
        text-align: inherit;
        background-color: transparent;
        text-decoration: none;
        font-size: 13px;
        text-transform: uppercase;
        font-weight: bold;
        color: #fff;
      }
    }
  }
</style>
